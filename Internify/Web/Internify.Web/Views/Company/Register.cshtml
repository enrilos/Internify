@model RegisterCompanyFormModel

@{
    ViewData["Title"] = "Register your company";
}

<section class="row">
    <section class="col-md-6 offset-md-3">
        <h2 class="heading-margin text-center">@ViewData["Title"]</h2>

        <form method="post">
            <partial name="_CompanyFormFieldsPartial" model="@Model" />
            <input class="btn btn-primary mt-3" type="submit" value="Submit" />
        </form>
    </section>
</section>

@section CKEditorCSS
{
    <link href="~/ckeditor/contents.css" ryle="stylesheet" />
}

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
    <script src="~/ckeditor/ckeditor.js"></script>
    <script>CKEDITOR.replace('editor');</script>
}